{ns hl7-fhir-r4-core.catalog,
 import #{zen.fhir
          hl7-fhir-r4-core.cqm-ValidityPeriod
          hl7-fhir-r4-core.value-set.catalogType
          hl7-fhir-r4-core.Composition
          hl7-fhir-r4-core.CatalogEntry},
 schema {:zen.fhir/version "0.16.13-2",
         :confirms #{hl7-fhir-r4-core.Composition/schema
                     zen.fhir/Resource},
         :zen/tags #{zen.fhir/profile-schema zen/schema},
         :zen.fhir/profileUri "http://hl7.org/fhir/StructureDefinition/catalog",
         :require #{:category :type :ValidityPeriod},
         :type zen/map,
         :zen/desc "A set of resources composed into a single coherent clinical statement with clinical attestation",
         :keys {:ValidityPeriod {:confirms #{hl7-fhir-r4-core.cqm-ValidityPeriod/schema},
                                 :fhir/extensionUri "http://hl7.org/fhir/StructureDefinition/cqm-ValidityPeriod",
                                 :zen/desc "The validity of the catalog"},
                :type {:const {:value {:text "Catalog"}},
                       :zen/desc "The type of document - a Catalog"},
                :category {:type zen/vector,
                           :every {:zen.fhir/value-set {:symbol hl7-fhir-r4-core.value-set.catalogType/value-set,
                                                        :strength :example},
                                   :zen/desc "The Content of the section"},
                           :minItems 1,
                           :maxItems 1},
                :subject {:confirms #{zen.fhir/Reference},
                          :zen.fhir/reference {:refers #{}},
                          :zen/desc "Who and/or what the composition is about"},
                :section {:type zen/vector,
                          :every {:type zen/map,
                                  :keys {:entry {:type zen/vector,
                                                 :every {:confirms #{zen.fhir/Reference},
                                                         :zen.fhir/reference {:refers #{hl7-fhir-r4-core.CatalogEntry/schema}},
                                                         :zen/desc "The Content of the section"},
                                                 :minItems 1}},
                                  :require #{:entry}}}},
         :zen.fhir/type "Composition"}}
